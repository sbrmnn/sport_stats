#!/usr/bin/env ruby
require 'sport_stats'
require 'file_obj'
require 'fastest-csv'

a = SportStats::HitterStats.new(FileObj::CSVObj.new("./assets/Batting-07-12.csv"))
b = SportStats::ConvertPlayerIdtoNames.new(a,"./assets/Master-small.csv")
puts "\n\n"
b.stat.batting_avg_list_out
puts "\n\n"
b.stat.slugging_pct_list_out
puts "\n\n"
b.stat.triple_crown_out
puts "\n\n"



